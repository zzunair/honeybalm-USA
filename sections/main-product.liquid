<style>
.vid-sec-{{ section.id }}{
  background-color: {{ section.settings.clr }};
}
.ing-tab-wrap.{{ section.id }}{
  /* background-color: {{ section.settings.ingbg }}; */
  background-color: white;
} 
</style>
{%- liquid
  render 'product-template', section: section
  assign current_variant = product.selected_or_first_available_variant
-%}
<script type="application/ld+json">
  {
    "@context": "http://schema.org/",
    "@type": "Product",
    "name": {{ product.title | json }},
    "url": {{ shop.url | append: product.url | json }},
    {%- if product.featured_media -%}
      {%- assign media_size = product.featured_media.preview_image.width | append: 'x' -%}
      "image": [
        {{ product.featured_media | image_url: width: media_size | prepend: "https:" | json }}
      ],
    {%- endif -%}
    "description": {{ product.description | strip_html | json }},
    {%- if current_variant.sku != blank -%}
      "sku": {{ current_variant.sku | json }},
    {%- endif -%}
    "brand": {
      "@type": "Thing",
      "name": {{ product.vendor | json }}
    },
    "offers": [
      {%- for variant in product.variants -%}
        {
          "@type" : "Offer",
          {%- if variant.sku != blank -%}
            "sku": {{ variant.sku | json }},
          {%- endif -%}
          "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
          "price" : {{ variant.price | divided_by: 100.00 | json }},
          "priceCurrency" : {{ cart.currency.iso_code | json }},
          "url" : {{ shop.url | append: variant.url | json }}
        }{% unless forloop.last %},{% endunless %}
    {%- endfor -%}
    ]
  }
  </script>

{% unless product.handle == 'hb-mystery-balm' or product.handle == 'hb-red-pepper' or product.handle == 'hb-lavender-chamomille' or product.handle == 'hb-strawberry-spf' or product.handle == 'spf-try-out-box'
  or product.handle == 'hb-lemon-cheesecake' %}
  <script>
    // var discount = thiss.options[thiss.selectedIndex].getAttribute('disc');
    // thiss.parentElement.querySelector('.prl-off').innerHTML = discount+'% off';
    // thiss.closest('.product-content').querySelector('.pdp-off ').innerHTML = discount+'% off';
    // document.querySelector('.product-content [name="id"]').value = thiss.value;
    // thiss.closest('.choose-wrap').querySelector('.pr-atc').innerHTML = thiss.options[thiss.selectedIndex].getAttribute('v-price');

    document.addEventListener("DOMContentLoaded", function(){
      var selects = document.querySelector('.prselect-wrap select');
        selects.addEventListener('change', function(){
          var discount = this.options[this.selectedIndex].getAttribute('disc');
          this.parentElement.querySelector('.prl-off').innerHTML = discount+'% off';
          this.closest('.product-content').querySelector('.pdp-off ').innerHTML = discount+'% off';
          document.querySelector('.sticky-atc-title .pdp-off').innerHTML = discount+'% off';
          if (discount == 0){
            this.closest('.product-content').querySelector('.pdp-off ').style.display="none";
            this.parentElement.querySelector('.prl-off').style.display="none";
            document.querySelector('.sticky-atc-title .pdp-off').style.display="none";
          } else{
            this.closest('.product-content').querySelector('.pdp-off ').style.display="block";
            this.parentElement.querySelector('.prl-off').style.display="block";
            document.querySelector('.sticky-atc-title .pdp-off').style.display="block";
          }
          // document.querySelector('.product-content [name="id"]').value = this.value;
          this.closest('.choose-wrap').querySelector('.pr-atc').innerHTML = this.options[this.selectedIndex].getAttribute('v-price');
          document.querySelector('.sticky-rts .pr-atc').innerHTML = this.options[this.selectedIndex].getAttribute('v-price');

        });
    });
    var tabs = document.querySelectorAll('.ing-tab-heading');
    for( var i = 0; i < tabs.length; i++){
      tabs[i].addEventListener("click", function(){
        this.parentElement.classList.toggle('active');
      });
    }



    window.addEventListener("load", function(){
      if(document.querySelector('.prselect-wrap [name="quantity"]')){
        var thiss = document.querySelector('.prselect-wrap [name="quantity"]');
        thiss.selectedIndex = 1;
        thiss.dispatchEvent(new Event('change'));
      }
    });

    

  </script>
  {% endunless %}

