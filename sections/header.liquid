{% if request.page_type == 'index' %}
  {% assign potential_action_target = shop.url | append: routes.search_url | append: "?q={search_term_string}" %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ shop.url | append: page.url | json }}
    }
  </script>
{% endif %}

<script>
  var childs = document.querySelectorAll('.child-mega');
  for(var i=0; i<childs.length; i++){
    childs[i].addEventListener('click', function(){
      document.querySelector('.child-mega.active').classList.remove('active');
      this.classList.add('active');
      var handle = this.getAttribute('handle');
      document.querySelector('.mega-p.active').classList.remove('active');
      document.querySelector('.mega-p.'+handle).classList.add('active');
    });
  }
</script>

