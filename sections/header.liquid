                            {{ child_link.title }} 
                          </a>
                        {%- endif -%}
                      {% endif %}
                    </li>
                  {%- endunless -%}
                {% endfor %}

                {% comment %}theme-check-disable LiquidTag{% endcomment %}
                {%- if have_feature_links and showcase_second_level_links -%}
                  {% assign features_mod = feature_link_count | modulo: 2 %}
                  {% for child_link in link.links %}
                      {%- if child_link.type == 'product_link' or child_link.type == 'collection_link' and child_link.levels == 0 -%}
                        {% if features_mod == 0 %}
                          {% capture cycle_class %}{% cycle "lg:col-end-11", "lg:col-end-13" %}{% endcapture %}
                        {% else %}
                          {% capture cycle_class %}{% cycle "lg:col-end-13", "lg:col-end-11" %}{% endcapture %}
                        {% endif %}
                        <li class="lg:col-span-2 {% unless all_feature_links %}{{ cycle_class }}{% endunless %} -mt-2">
                          {%- assign object = child_link.object -%}
                          {% render 'showcase-tile' with object: object, crop: crop %}
                        </li>
                      {%- endif -%}
                  {% endfor %}
                {%- endif -%}
                {% comment %}theme-check-enable LiquidTag{% endcomment %}
              </ul>
              {% else %}
                {%- render 'desktop-nav-featured-collection-links', link: link, crop: crop -%}
              {% endunless %}
                {% if link.title == 'Explore' %}
                  <div class="explore">
                    <div class="get">
                      <div class="get-title heading4 strong">
                        Get rewards and enjoy Honeybalm even more!
                      </div>
                      <div class="get-txt base normal">
                        See the rewards for yourself
                      </div>
                      <a class="exp-shop" href="/pages/rewards">
                        <span class="push-btn-surface w-full outline-btn">Start now 
                          <svg xmlns="http://www.w3.org/2000/svg" width="15" height="16" viewBox="0 0 15 16" fill="none">
                          <path d="M7.3125 1.21875C7.4375 1.0625 7.6875 1.0625 7.84375 1.21875L14.375 7.75C14.5312 7.90625 14.5312 8.125 14.375 8.28125L7.84375 14.8125C7.6875 14.9688 7.4375 14.9688 7.3125 14.8125L6.6875 14.2188C6.53125 14.0625 6.53125 13.8125 6.6875 13.6875L11.5312 8.8125H0.875C0.65625 8.8125 0.5 8.65625 0.5 8.4375V7.5625C0.5 7.375 0.65625 7.1875 0.875 7.1875H11.5312L6.6875 2.34375C6.53125 2.21875 6.53125 1.96875 6.6875 1.8125L7.3125 1.21875Z" fill="black"></path>
                          </svg>
                        </span>
                      </a>
                    </div>
                    <div class="build">
                      <div class="get-title heading4 strong">
                        Build your own bundle
                      </div>
                      <div class="get-txt base normal">
                        Get up to 52% discount!
                      </div>
                      <a class="exp-shop" href="/pages/bundle-builder">
                        <span class="push-btn-surface w-full outline-btn">Shop now 
                          <svg xmlns="http://www.w3.org/2000/svg" width="15" height="16" viewBox="0 0 15 16" fill="none">
                          <path d="M7.3125 1.21875C7.4375 1.0625 7.6875 1.0625 7.84375 1.21875L14.375 7.75C14.5312 7.90625 14.5312 8.125 14.375 8.28125L7.84375 14.8125C7.6875 14.9688 7.4375 14.9688 7.3125 14.8125L6.6875 14.2188C6.53125 14.0625 6.53125 13.8125 6.6875 13.6875L11.5312 8.8125H0.875C0.65625 8.8125 0.5 8.65625 0.5 8.4375V7.5625C0.5 7.375 0.65625 7.1875 0.875 7.1875H11.5312L6.6875 2.34375C6.53125 2.21875 6.53125 1.96875 6.6875 1.8125L7.3125 1.21875Z" fill="black"></path>
                          </svg>
                        </span>
                      </a>
                    </div>
                  </div>
                {% endif %}
                {% if link.title == 'Shop' %}
                  {% for child_link in link.links %}


                    {% if child_link.title == 'Accessories' %}
                      {% if child_link.links != blank %}
                        <ul class="{{ child_link.title | handle }} mega-p {% if forloop.index == 1 %} active {% endif %}">
                          {% for pro in collections['applicators'].products %}
                            <li>
                              <a {% if grandchild_link.active %}aria-current="page"{% endif %} href="{{ pro.url }}" class="{{ tertiary_link_classes }}">
                                <!-- { % assign pro_handle = grandchild_link.url | split: '/products/' | last %}
                                { % assign pro = all_products[pro_handle] %} -->
                                <div class="menu-img-wrap {{ pro.title | downcase }}">
                                    <img src="{{ pro.images[0].src | img_url: '300x' }}"/>
                                </div>
                                <div class="mega-title heading4 strong">
                                  {{ pro.title }}
                                </div>
                                    <div class="mega-text base normal"></div>
                                <!-- <div class="mega-text base normal">Lip balm</div> -->
                              </a>
                            </li>
                          {% endfor %}
                        </ul>
                       {% endif %}
                      {% continue %}
                    {% endif %}

                    
                    {% if child_link.links != blank %}
                    <ul class="{{ child_link.title | handle }} mega-p {% if forloop.index == 1 %} active {% endif %}">
                      {% for grandchild_link in child_link.links %}
                        <li>
                          <a {% if grandchild_link.active %}aria-current="page"{% endif %} href="{{ grandchild_link.url }}" class="{{ tertiary_link_classes }}">
                            {% assign pro_handle = grandchild_link.url | split: '/products/' | last %}
                            <!-- { % assign pro = all_products[pro_handle] %} -->
                            {% assign pro = grandchild_link.object %}
                            <div class="menu-img-wrap {{ grandchild_link.title | downcase }}">
                              {% if grandchild_link.title == "SPF Try-out box" or grandchild_link.title == "Mystery Balm" or grandchild_link.title == "Salted Caramel" or grandchild_link.title == "Red Pepper" 
                                 or grandchild_link.title == "Hibiscus Citrus" or grandchild_link.title == "Lemon Cheesecake" or grandchild_link.title == "Candy Cane" or grandchild_link.title == "Honey lip scrub" 
                                 or grandchild_link.title == "Blueberry" or grandchild_link.title == "Pineapple" or grandchild_link.title == "Rose Cardamom" %} 
                                <img src="{{ pro.images[1].src | img_url: '300x' }}"/>
                              {% elsif grandchild_link.title == "Try-out box" or grandchild_link.title contains "Valentine" or grandchild_link.title == "Classic Pack" or grandchild_link.title == "Favourites Pack" 
                                or grandchild_link.title == "Lavender Chamomille" %} 
                                <img src="{{ pro.images[0].src | img_url: '300x' }}"/>
                              {% elsif child_link.title == 'Packs' %}
                                <img src="{{ pro.images[0].src | img_url: '300x' }}"/>                              
                              {% elsif grandchild_link.title == "Watermelon" or grandchild_link.title == "Strawberry lip scrub" %}
                                <img src="{{ pro.images[3].src | img_url: '300x' }}"/>
                              {% else %}
                                <img src="{{ pro.images[6].src | img_url: '300x' }}"/>
                              {% endif %}
                              {% if grandchild_link.title == "Strawberry lip scrub" or grandchild_link.title == "Rose Cardamom" or grandchild_link.title == "Salted Caramel"
                                 or grandchild_link.title == "Hibiscus Citrus" %}
                                <div class="menu-tag base-sm strong  new">New</div>
                              {% elsif grandchild_link.title == "Honey SPF" or grandchild_link.title == "Coconut SPF" or grandchild_link.title == "Watermelon" or grandchild_link.title == "Mint" or grandchild_link.title == "Try-out box" or grandchild_link.title == "Blueberry" %}
                                <div class="menu-tag base-sm strong best-seller">Best seller</div>
                              {% endif %}
                              {% if grandchild_link.title == "Mystery Balm" or grandchild_link.title == "Red Pepper" or grandchild_link.title == "SPF Try-out box" or grandchild_link.title == "Strawberry SPF" 
