{%- capture cart_icon %}
  {%- if show_icons -%}
    <div class="relative my-auto align-middle">
      <span class="inline-block w-6 h-6 fill-current align-top">
        {%- if settings.cart_icon == 'cart' -%}
          {%- render 'icon-cart' -%}
        {%- else -%}
          {%- render 'icon-bag' -%}
        {%- endif -%}
        <span class="visually-hidden">{{ 'layout.cart.title' | t }} </span>
      </span>
      {% comment %}
      {%- if cart != empty -%}
      <span x-show="$store.cart_count.count > 0" class="c-count tracking-normal absolute -bottom-1.5 -right-1.5 w-5 h-5 bg-scheme-text text-scheme-background rounded-full text-[0.625rem] flex items-center justify-center" x-text="$store.cart_count.count">{{ cart.item_count }}</span>
      {%- endif -%}
      {% endcomment %}
      <!-- { %- if cart != empty -%} -->
        <a href="/cart" id="cart-icon-bubble">
          <div class="cart-count-bubble">
            <span class="c-count tracking-normal absolute -bottom-1.5 -right-1.5 w-5 h-5 bg-scheme-text text-scheme-background rounded-full text-[0.625rem] flex items-center justify-center">{{ cart.item_count }}</span>
          </div>
        </a>
      <!-- { %- endif -%} -->
      <span class="visually-hidden">
        {%- if cart.item_count == 1 -%}
          {{- 'layout.cart.items_count.one' | t -}}
        {%- else -%}
          {{- 'layout.cart.items_count.other' | t -}}
        {%- endif -%}
      </span>
    </div>
  {%- else -%}
    {{ 'layout.cart.title' | t }} (<span  x-text="$store.cart_count.count">{{ cart.item_count }}</span>)
    <span class="visually-hidden">
      {%- if cart.item_count == 1 -%}
        {{- 'layout.cart.items_count.one' | t -}}
      {%- else -%}
        {{- 'layout.cart.items_count.other' | t -}}
      {%- endif -%}
    </span>
  {%- endif -%}
{%- endcapture -%}

{%- capture cart_button -%}
  {% if settings.cart_type == 'modal' and template != 'cart' %}
    <button
      class="csm-icon js-enabled whitespace-nowrap {% if show_icons %}{{ icon_link_classes }}{% else %}{{ link_classes }}{% endif %}"
      type="button"
      id="CartButton"
      @click.prevent="$store.modals.open('cart')"
      >
      {{ cart_icon }}
      
      <span class="icon-text">Bag</span>
    </button>
    {% comment %}
    <noscript>
      <a
        class="whitespace-nowrap {{ link_classes }}"
        href="{{ routes.cart_url }}"
      >
        {{ cart_icon }}
      </a>
    </noscript>
    {% endcomment %}
  {% else %}
    <a
      class="whitespace-nowrap {{ link_classes }}"
      href="{{ routes.cart_url }}"
    >
      {{ cart_icon }}
    </a>
  {% endif %}
{%- endcapture -%}

{% comment %}
  Accounts
{% endcomment %}
{%- capture accounts -%}
  {% if shop.customer_accounts_enabled %}
    {% if customer %}
      <a class="csm-icon {% if show_icons %}{{ icon_link_classes }}{% else %}{{ link_classes }}{% endif %}" href="{{ routes.account_url }}">
        <span class="{% if show_icons %}visually-hidden{% endif %}">{{ 'layout.customer.account' | t }}</span>
        {%- if show_icons -%}
        <span class="inline-block w-6 h-6 align-middle">{%- render 'icon-account' -%}</span>
      {%- endif -%}
      <span class="icon-text">Account</span>
      </a>
    {% else %}
      <div class="ligin-wrap">
        <a class="csm-icon csm-login {% if show_icons %}{{ icon_link_classes }}{% else %}{{ link_classes }}{% endif %}" href="{{ routes.account_login_url }}">
